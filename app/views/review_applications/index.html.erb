 <div class='row'>
  <div class='member-applications span5 offset1'>
    <h3>Member Applications</h3>
    <% if @member_applications.any? %>
      <% @member_applications.each do |application| %>
        <%= link_to @member_applicants[application.user_uuid].name, member_application_path(application) %>
      <% end %>
    <% end %>
  </div>
  <div class='mentor-applications span5 offset1'>
    <h3>Mentor Applications</h3>
    <% if @mentor_applications.any? %>
      <% @mentor_applications.each do |application| %>
        <%= link_to @mentor_applicants[application.user_uuid].name, mentor_application_path(application) %>
      <% end %>
    <% end %>
  </div>
</div>
<div class="row">
  <%= link_to 'Review Rejected Applications', rejected_applications_index_path, class: "btn btn-primary offset4" %>
</div>
